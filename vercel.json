{
  "outputDirectory": "build/docs",
  "rewrites": [
    {
      "source": "/docs/:asset*",
      "destination": "/:asset*"
    },
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    {
      "source": "/docs/getting_started",
      "destination": "/docs/",
      "permanent": true
    }
  ],
  "github": {
    "silent": true
  }
}
